# Development Environment
NODE_ENV=development
DEBUG=true

# Local Database
DATABASE_URL=postgresql://postgres:password@localhost:5432/xm_port_dev
DATABASE_URL_ASYNC=postgresql+asyncpg://postgres:password@localhost:5432/xm_port_dev

# Local Redis
REDIS_URL=redis://localhost:6379/0

# Development Security (less strict)
SECRET_KEY=development-secret-key-at-least-32-chars-long-change-in-production
NEXTAUTH_SECRET=development-nextauth-secret

# Enable development features
ENABLE_SWAGGER=true
ENABLE_METRICS=true
LOG_LEVEL=DEBUG